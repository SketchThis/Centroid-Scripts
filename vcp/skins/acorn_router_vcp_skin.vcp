<vcp_skin>
	<border>
		<column_span>2</column_span>
		<column_start>1</column_start>
		<fill>#00007F</fill>
		<row_span>4</row_span>
		<row_start>1</row_start>
		<outline_color>#000000</outline_color>
		<outline_thickness>2</outline_thickness>
